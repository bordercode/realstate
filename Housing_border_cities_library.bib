@article{nowakAssessmentNutritionalComposition2016,
  title = {Assessment of the Nutritional Composition of Quinoa ({{Chenopodium}} Quinoa {{Willd}}.)},
  author = {Nowak, Verena and Du, Juan and Charrondière, U. Ruth},
  year = {2/15/ 2016},
  journaltitle = {Food Chemistry},
  volume = {193},
  pages = {47--54},
  issn = {0308-8146},
  doi = {http://dx.doi.org/10.1016/j.foodchem.2015.02.111},
  abstract = {Quinoa (Chenopodium quinoa Willd.) is an ancient crop which can play an important role for worldwide food security. The current review aimed at evaluating existing compositional data which were compiled according to international standards. A limited number of data were found that met the dataset quality criteria. In general, high variations in nutrient contents of quinoa were observed per 100 g edible portion on fresh weight basis, for example: protein (9.1–15.7 g), total fat (4.0–7.6 g) and dietary fiber (8.8–14.1 g). The variations of nutrient values among different varieties and among different data sources were considerable. The results show the nutritional potential of quinoa but they also demonstrate that more high-quality analytical data of quinoa are needed, especially for minerals and vitamins.},
  keywords = {Chenopodium quinoa,FAO,Food biodiversity,Food composition,Nutrient content,Pseudocereal}
}

