@article{gathmannEffectsEnforcementIllegal2008,
  title = {Effects of Enforcement on Illegal Markets: {{Evidence}} from Migrant Smuggling along the Southwestern Border},
  author = {Gathmann, Christina},
  date = {2008-10-01},
  journaltitle = {Journal of Public Economics},
  shortjournal = {Journal of Public Economics},
  volume = {92},
  number = {10},
  pages = {1926--1941},
  issn = {0047-2727},
  doi = {10.1016/j.jpubeco.2008.04.006},
  url = {http://www.sciencedirect.com/science/article/pii/S0047272708000674},
  abstract = {The paper analyzes the effects of tighter border control on the illegal crossing market between Mexico and the United States. Combining panel data on illegal migrants with enforcement statistics, the effects of enforcement are found to be moderate: prices of border smugglers (‘coyotes’) increased by only 17\%, while the demand for smugglers has remained unchanged. Enforcement has however shifted illegal migrants to remote crossing places. Border crossing is now more time-intensive with higher prices for coyotes and risk of death. This geographic substitution raised migration costs by \$140, more than twice the effect of enforcement on smuggling prices.},
  keywords = {Crime,Enforcement,Illegal migration,Mexico,Smuggling}
}

