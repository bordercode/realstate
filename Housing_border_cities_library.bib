@article{byrneTestingAlternativeDefinitions2015,
  title = {Testing {{Alternative Definitions}} of {{Chronic Homelessness}}.},
  author = {Byrne, Thomas and Culhane, Dennis P.},
  date = {2015-09},
  journaltitle = {Psychiatric services (Washington, D.C.)},
  shortjournal = {Psychiatr Serv},
  volume = {66},
  number = {9},
  eprint = {25828880},
  eprinttype = {pmid},
  pages = {996--999},
  location = {{United States}},
  issn = {1557-9700 1075-2730},
  doi = {10.1176/appi.ps.201400240},
  abstract = {OBJECTIVE: This study examined the potential impact of a proposed change to the official federal definition of chronic homelessness. METHODS: Using  administrative data from the emergency shelters in a large U.S. city, this study  estimated the number of persons identified as chronically homeless under the  current definition of chronic homelessness, a proposed new federal definition,  and two alternative definitions and examined shelter utilization for each group.  RESULTS: Fewer than half as many people were considered chronically homeless  under the proposed new federal definition compared with the current definition.  Persons considered chronically homeless by the proposed new definition and, to a  lesser extent, by the two alternative definitions, made heavier use of shelter  compared with persons who met the current definition. CONCLUSIONS: A proposed new  and two alternative definitions of chronic homelessness are better suited than  the existing federal definition for identifying persons with the most protracted  experiences of homelessness.},
  langid = {english},
  keywords = {Humans,Ill-Housed Persons/*statistics \& numerical data,United States,Urban Population/statistics \& numerical data}
}

