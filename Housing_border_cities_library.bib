@article{kolvesChoiceSuicideMethod2018,
  title = {Choice of a Suicide Method: {{Trends}} and Characteristics},
  author = {Kõlves, Kairi and McDonough, Matthew and Crompton, David and family=Leo, given=Diego, prefix=de, useprefix=true},
