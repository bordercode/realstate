@article{lustigSALUDDESARROLLOECONOMICO2007,
  title = {{{SALUD Y DESARROLLO ECONÓMICO}}: {{El}} Caso de {{México}}},
  author = {Lustig, Nora},
  date = {2007},
  journaltitle = {El Trimestre Económico},
  volume = {74},
  eprint = {20857137},
  eprinttype = {jstor},
  pages = {793--822},
  issn = {00413011},
  url = {http://www.jstor.org/stable/20857137},
  abstract = {En este art\&\#xed;culo se analiza la relaci\&\#xf3;n entre salud, crecimiento econ\&\#xf3;mico y pobreza, los niveles, desigualdad y evoluci\&\#xf3;n de los indicadores de salud, as\&\#xed; como la equidad y eficiencia de la acci\&\#xf3;n p\&\#xfa;blica en materia de salud en M\&\#xe9;xico. Este pa\&\#xed;s tiene niveles promedio de salud inferiores a los esperados para su desarrollo y, aunque en general han mejorado con el tiempo, lo han hecho a un ritmo menor al adecuado. Existen tambi\&\#xe9;n grandes contrastes entre zonas geogr\&\#xe1;ficas, grupos \&\#xe9;tnicos y niveles socioecon\&\#xf3;micos. La desigualdad en los indicadores es reflejo de la desigualdad en la acci\&\#xf3;n p\&\#xfa;blica, ya que el gasto p\&\#xfa;blico en salud no es progresivo y ha mantenido a cerca de 50\% de la poblaci\&\#xf3;n fuera de los sistemas de seguridad p\&\#xfa;blica. Asimismo, se concluye que para alcanzar la eficiencia adecuada, el sistema de seguridad social deber\&\#xed;a establecer un fondo \&\#xfa;nico nacional para financiar intervenciones m\&\#xe9;dicas espec\&\#xed;ficas y esquemas complementarios para servicios m\&\#xe9;dicos adicionales; permitir a m\&\#xfa;ltiples instituciones, tanto p\&\#xfa;blicas como privadas, proporcionar los servicios de salud cubiertos por dicho fondo \&\#xfa;nico para incrementar el acceso y competencia, as\&\#xed; como garantizar la portabilidad de derechos, para el caso, por ejemplo, en que el asegurado se traslade a otra zona geogr\&\#xe1;fica o cambie de situaci\&\#xf3;n laboral. This article analizes the relationship between health and economic growth and poverty. It finds that Mexico presents lower levels of health than those expected for its degree of development. It also concludes that there are huge contrasts among geographic zones, ethnic groups and socioeconomic status. This may be due to a somehow regressive public expenditure in health as half the population is not covered by any social security scheme. Hence it is needed a National Fund to finance healcare and allow a universal system that is independent of, for example, geographic zone or public/private source of work.},
  issue = {296(4)}
}

