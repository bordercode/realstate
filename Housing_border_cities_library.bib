@article{choiHeterogeneousEffectsInteractions2022,
  title = {The Heterogeneous Effects of Interactions between Parent's Education and {{MSA}} Level College Share on Children's School Enrollment},
  author = {Choi, Jung Hyun and Green, Richard K.},
  date = {2022-09-01},
  journaltitle = {Journal of Housing Economics},
  shortjournal = {Journal of Housing Economics},
  volume = {57},
  pages = {101843},
  issn = {1051-1377},
  doi = {10.1016/j.jhe.2022.101843},
  url = {https://www.sciencedirect.com/science/article/pii/S1051137722000171},
  abstract = {This study finds that location matters more for children of parents with low educational attainment than for children of more educated parents. Specifically, children of parents without a high school diploma are statistically more likely to be enrolled in high schools or colleges in metropolitan statistical areas (MSAs) where the college graduate share is higher. The statistical relationship between a child's school enrollment and the MSA's college graduate share becomes weaker as their parents’ educational attainment increases. We also find that the least educated households are significantly less likely to be homeowners and are more likely to pay greater housing costs as a share of income in cities with higher college graduate shares, indicating that these households are paying a high price to live in high-skilled cities. Our results imply that the increasing lack of housing affordability in high-skilled cities makes it more difficult for households with less educational attainment to stay in or move to these places, contributing to intergenerational education inequality.},
  keywords = {High-skilled city,Housing affordability,Intergenerational mobility,Parent education,School enrollment}
}

