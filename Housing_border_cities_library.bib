@article{CHITEWERE2017117,
  title = {How {{Neighborhoods Influence Health}}: {{Lessons}} to Be Learned from the Application of Political Ecology},
  author = {Chitewere, Tendai and Shim, Janet K. and Barker, Judith C. and Yen, Irene H.},
  date = {2017},
  journaltitle = {Health \& Place},
  volume = {45},
  pages = {117--123},
  issn = {1353-8292},
  doi = {10.1016/j.healthplace.2017.03.009},
  url = {http://www.sciencedirect.com/science/article/pii/S1353829217302241},
  abstract = {AIM This paper articulates how political ecology can be a useful tool for asking fundamental questions and applying relevant methods to investigate structures that impact relationship between neighborhood and health. Through a narrative analysis, we identify how political ecology can develop our future agendas for neighborhood-health research as it relates to social, political, environmental, and economic structures. Political ecology makes clear the connection between political economy and neighborhood by highlighting the historical and structural processes that produce and maintain social inequality, which affect health and well-being. These concepts encourage researchers to examine how people construct neighborhood and health in different ways that, in turn, can influence different health outcomes and, thus, efforts to address solutions.},
  keywords = {Neighborhood,Political ecology}
}

