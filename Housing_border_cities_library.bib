@incollection{schlimmePhenomenologicalApproachSuicidal2018,
  title = {A {{Phenomenological Approach}} to {{Suicidal Mental Life}}},
  booktitle = {Phenomenology of {{Suicide}}: {{Unlocking}} the {{Suicidal Mind}}},
  author = {Schlimme, Jann E.},
  editor = {Pompili, Maurizio},
  date = {2018},
  pages = {31--37},
  publisher = {{Springer International Publishing}},
  location = {{Cham}},
  doi = {10.1007/978-3-319-47976-7_3},
  url = {https://doi.org/10.1007/978-3-319-47976-7_3},
  abstract = {A phenomenology of suicidal mental life describes this mental condition as a condition with two poles: on one side, one’s life forecasts ongoing and unbearable despair and, on the other side, one’s self-inflicted death promises relief. A phenomenological description stresses both the fact that there is no suicidal mental life without the experience of relief (rescue, remedy) in the option to kill oneself and that there is no experienced relief in this option without despair. This two-faced condition results in extreme tension and ambivalence, usually displayed in interpersonal relationships (i.e. calling for help through attempting suicide). It is furthermore necessary to acknowledge the possible cathartic effect of the knowledge of one’s option to suicide. This catharsis leads out of a life-endangering condition, but may need support nonetheless. For a fine-grained phenomenological understanding, it is required to take both sides of suicidal mental life into account.},
  isbn = {978-3-319-47976-7}
}

